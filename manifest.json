{
    "name": "Sonatype Nexus IQ Evaluation",
    "description": "Sonatype Nexus IQ Evaluation",
    "author": "Cameron Townshend - cameron.townshend@sonatype.com",
    "version": "1.7.1",
    "manifest_version": 2,
    "permissions": [
      "storage",
      "declarativeContent",
      "webRequest",
      "webRequestBlocking", 
      "cookies", 
      "*://iq-server:*/",
      "background",
      "tabs", 
      "https://search.maven.org/",
      "https://mvnrepository.com/",
      "https://www.npmjs.com/",
      "https://www.nuget.org/",
      "https://rubygems.org/",
      "https://pypi.org/",
      "https://packagist.org/",
      "https://ossindex.sonatype.org/"

      ],
    "background": {
      "scripts": ["Scripts/jquery-3.3.1.min.js", "Scripts/shared.js", "Scripts/background.js"],
      "persistent": true
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
         "16": "images/IQ_Default.png",
         "32": "images/IQ_Default.png",
         "48": "images/IQ_Default.png",
         "128": "images/IQ_Default.png"
       }
     }, 
    // "browser_action":{
    //     "default_popup": "popup.html",
    //     "default_title": "Popup test"
    // },
    "icons": {
      "16": "images/IQ_Default.png",
      "32": "images/IQ_Default.png",
      "48": "images/IQ_Default.png",
      "128": "images/IQ_Default.png"
    },
    "content_scripts":[
      {
        "matches": ["https://search.maven.org/artifact/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },
      {
        "matches": ["https://mvnrepository.com/artifact/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },

      {
        "matches": ["https://www.npmjs.com/package/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },
      {
        "matches": ["https://www.nuget.org/packages/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },
      {
        "matches": ["https://pypi.org/project/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },
      {  
        "matches": ["https://rubygems.org/gems/*"],
        "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      },
      {          
       "matches": ["https://packagist.org/packages/*"],
       "js": ["Scripts/jquery-3.3.1.min.js", "Scripts/content.js"]
      }                
    ],
  "options_page": "options.html"    
  }
  